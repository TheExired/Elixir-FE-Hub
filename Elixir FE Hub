-- Module Scripts:

local fake_module_scripts = {}

do -- Aerethus.CircleClick
	local script = Instance.new('ModuleScript', Aerethus)
	script.Name = "CircleClick"
	local function module_script()
		function CircleClick(Button, X, Y)
			coroutine.resume(coroutine.create(function()
				
				Button.ClipsDescendants = true
				
				local Circle = script:WaitForChild("Circle"):Clone()
					Circle.Parent = Button
					local NewX = X - Circle.AbsolutePosition.X
					local NewY = Y - Circle.AbsolutePosition.Y
					Circle.Position = UDim2.new(0, NewX, 0, NewY)
				
				local Size = 0
					if Button.AbsoluteSize.X > Button.AbsoluteSize.Y then
						 Size = Button.AbsoluteSize.X*1.5
					elseif Button.AbsoluteSize.X < Button.AbsoluteSize.Y then
						 Size = Button.AbsoluteSize.Y*1.5
					elseif Button.AbsoluteSize.X == Button.AbsoluteSize.Y then																										Size = Button.AbsoluteSize.X*1.5
					end
				
				local Time = 0.5
					Circle:TweenSizeAndPosition(UDim2.new(0, Size, 0, Size), UDim2.new(0.5, -Size/2, 0.5, -Size/2), "Out", "Quad", Time, false, nil)
					for i=1,10 do
						Circle.ImageTransparency = Circle.ImageTransparency + 0.01
						wait(Time/10)
					end
					Circle:Destroy()
					
			end))
		end
		
		return CircleClick
	end
	fake_module_scripts[script] = module_script
end
do -- Button.MainModule
	local script = Instance.new('ModuleScript', Button)
	script.Name = "MainModule"
	local function module_script()
		local ChangeValue = {}
		activated = false
		function ChangeValue:CV(player, toggle)
			player = game.Players:WaitForChild(player)
			local SS = player.PlayerGui.SynapseHub
			local Config = SS.Main.Config
			
			if activated == false then
				activated = true
				script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Config[script.Parent.Parent.Parent.Parent.Parent.Name].Value = true
			elseif activated == true then
				activated = false
				script.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Parent.Config[script.Parent.Parent.Parent.Parent.Parent.Name].Value = false
				end
		end
		
		return ChangeValue
		
	end
	fake_module_scripts[script] = module_script
end


-- Scripts:

local function MKZHX_fake_script() -- OpenClose.LocalScript 
	local script = Instance.new('LocalScript', OpenClose)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local closed = false
	script.Parent.MouseButton1Down:Connect(function()
		if closed == false then
			script.Parent.Parent.Parent.Main2.Visible = false
			script.Parent.Parent.Parent.Sidebar.Visible = false
			script.Parent.Parent.Parent.TabsHolder.Visible = false
			script.Parent.Text = "+"
			closed = true
		elseif closed == true then
			script.Parent.Parent.Parent.Main2.Visible = true
			script.Parent.Parent.Parent.Sidebar.Visible = true
			script.Parent.Parent.Parent.TabsHolder.Visible = true
			script.Parent.Text = "-"
			closed = false
		end
	end)
end
coroutine.wrap(MKZHX_fake_script)()
local function HRHM_fake_script() -- Buttons.LocalScript 
	local script = Instance.new('LocalScript', Buttons)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local tabs = script.Parent.Parent.Parent.TabsHolder
	
	for i,v in pairs(script.Parent:GetChildren()) do
		if v:IsA("TextButton") then
			v.MouseButton1Click:Connect(function()
				if tabs:FindFirstChild(v.Name) then
					tabs:FindFirstChild(v.Name):TweenPosition(UDim2.new(-0.002, 0,-0.006, 0),"Out","Quint", 0.7, true) -- position of your first page like home or something
					for a,e in pairs(tabs:GetChildren()) do
						if e.Name ~= v.Name then
							e:TweenPosition(UDim2.new(0.998, 0,-0.003, 0),"Out","Quint", 0.5, true) -- position of the other pages for example executor or script hub
						end
					end
				end
			end)
		end
	end
end
coroutine.wrap(HRHM_fake_script)()
local function IXPU_fake_script() -- TextLabel_2.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_2)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	
	script.Parent.Text = "Welcome to Elixir Hub " .. game.Players.LocalPlayer.Name .. "!"
end
coroutine.wrap(IXPU_fake_script)()
local function MUNIHNJ_fake_script() -- nil.CircleClickExample 
	local script = Instance.new('LocalScript', nil)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local Mouse = game.Players.LocalPlayer:GetMouse()
	local CircleClick = require(script.Parent)
	
	script.Parent.Parent.MouseButton1Down:connect(function()
		CircleClick(script.Parent.Parent, Mouse.X, Mouse.Y) 
	end)
	
	
	
end
coroutine.wrap(MUNIHNJ_fake_script)()
local function KMQNEB_fake_script() -- Aerethus.LocalScript 
	local script = Instance.new('LocalScript', Aerethus)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	
	local function FEScript()
	
	
		local unanchoredparts = {}
		local movers = {}
		local toggle = true
		local move = false
		local Player = game:GetService("Players").LocalPlayer
		local Character = Player.Character
		local mov = {};
		local mov2 = {};
	
		--//Settings//--
	
		FlingStrength = 16000
	
		game:GetService("RunService").RenderStepped:Connect(function()
			Character.Head.CanCollide = false
			Character.Torso.CanCollide = false
			Character.HumanoidRootPart.CanCollide = false
	
		end)
	
		local runservice = game:GetService("RunService")
	
	
	
	
	
	
		local Hats = {Hat1  = Character:WaitForChild("DualVoidScythe"),
			Hat2  = Character:WaitForChild("Evil Aura"), 
			Hat3  = Character:WaitForChild("Hat1"), 
			Hat4  = Character:WaitForChild("Kate Hair"), 
			Hat5  = Character:WaitForChild("LavanderHair"), 
			Hat6  = Character:WaitForChild("MessyHair"), 
			Hat7  = Character:WaitForChild("Pal Hair"), 
			Hat8  = Character:WaitForChild("Pink Hair"), 
			Hat9  = Character:WaitForChild("Starslayer Railgun"), 
			Hat10  = Character:WaitForChild("Big Block"),
		}
	
		local temp = Instance.new("BodyPosition")
		temp.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
		temp.Parent = Hats.Hat10.Handle
	
		local Fling = Instance.new("BodyThrust")
		Fling.Parent = Hats.Hat10.Handle
