--//Key Scripts//--


confirm.MouseButton1Down:Connect(function()
	if textbox.Text == "ElixirFETestKey" then
		keye = true
		sidebar.Visible = true
		tabsholder.Visible = true
		keyframe.Visible = false
	end
end)


























--//Button Scripts//--

local tabs = tabsholder

for i,v in pairs(buttonside:GetChildren()) do
	if v:IsA("TextButton") then
		v.MouseButton1Click:Connect(function()
			if tabs:FindFirstChild(v.Name) then
				tabs:FindFirstChild(v.Name):TweenPosition(UDim2.new(-0.077,0,-0.003,0),"Out","Quint", 0.7, true) -- position of your first page like home or something
				for a,e in pairs(tabs:GetChildren()) do
					if e.Name ~= v.Name and not e:IsA("UICorner") then
						e:TweenPosition(UDim2.new(0.998, 0,-0.003, 0),"Out","Quint", 0.5, true) -- position of the other pages for example executor or script hub
					end
				end
			end
		end)
	end
end




closez.MouseButton1Down:Connect(function()
	
	if closed == true then
		if keye == true then
			main.Visible = true
			sidebar.Visible = true
			tabsholder.Visible = true
			keyframe.Visible = false
			closez.Text = "+"
			closed = false
		elseif keye == false then
			main.Visible = true
			sidebar.Visible = false
			tabsholder.Visible = false
			keyframe.Visible = true
			closez.Text = "+"
			closed = false
			end
	elseif closed == false then
			main.Visible = false
			sidebar.Visible = false
			tabsholder.Visible = false
			keyframe.Visible = false
			closez.Text = "-"
			closed = true
		end
end)







