coroutine.wrap(KMQNEB_fake_script)()
local function BNQTK_fake_script() -- ImageButton.LocalScript 
	local script = Instance.new('LocalScript', ImageButton)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	script.Parent.Parent.TextLabel.Visible = true
	local function tweentransparency1()
		local Info = TweenInfo.new(0.25)
		local Tween = game:GetService("TweenService"):Create(script.Parent.Parent.TextLabel,Info,{BackgroundTransparency=1})
		Tween:Play()
	end
	
	
	local function tweentransparency0()
		local Info = TweenInfo.new(0.25)
		local Tween = game:GetService("TweenService"):Create(script.Parent.Parent.TextLabel,Info,{BackgroundTransparency=0})
		Tween:Play()
	end
	
	local Description = false
	local oldtext = script.Parent.Parent.Text
	script.Parent.MouseButton1Down:Connect(function()
		if Description == false then
			Description = true
			tweentransparency0()
		elseif Description == true then
			Description = false
			tweentransparency1()
			wait(2)
		end
	end)
end
coroutine.wrap(BNQTK_fake_script)()
